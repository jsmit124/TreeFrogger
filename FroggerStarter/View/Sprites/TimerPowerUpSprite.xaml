        <Rectangle Fill="{StaticResource SecondaryColor}" Width="6" Height="3" Canvas.Top="6" Canvas.Left="9" />
        <Rectangle Fill="{StaticResource MainColor}" Width="20" Height="38" Canvas.Top="6" Canvas.Left="15" />
