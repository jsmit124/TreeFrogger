        <Rectangle Fill="{StaticResource Border}" Width="3" Height="6" Canvas.Top="35" Canvas.Left="3" />
        <Rectangle Fill="{StaticResource Border}" Width="3" Height="6" Canvas.Top="9" Canvas.Left="3" />
        <Rectangle Fill="{StaticResource Border}" Width="3" Height="3" Canvas.Top="6" Canvas.Left="6" />
